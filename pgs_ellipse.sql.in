   RETURNS float8
   AS 'MODULE_PATHNAME' , 'sphereellipse_rad1'
   LANGUAGE 'c'
   WITH (isstrict,iscachable);

COMMENT ON FUNCTION lrad(sellipse) IS
  'large radius of spherical ellipse'; 

