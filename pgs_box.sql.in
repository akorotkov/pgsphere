COMMENT ON FUNCTION nw(sbox) IS
  'north-west corner of spherical box'; 

CREATE FUNCTION ne(sbox)
   RETURNS spoint
   AS 'MODULE_PATHNAME' , 'spherebox_ne'
   LANGUAGE 'c'
   IMMUTABLE STRICT ;

COMMENT ON FUNCTION ne(sbox) IS
