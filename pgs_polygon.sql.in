CREATE FUNCTION circum(spoly)
   RETURNS FLOAT8
   AS 'MODULE_PATHNAME', 'spherepoly_circ'
   LANGUAGE 'c'
   IMMUTABLE STRICT;

COMMENT ON FUNCTION  circum(spoly) IS
  'returns circumference of spherical polygon'; 

CREATE OPERATOR  @-@ (
   RIGHTARG   = spoly,
   PROCEDURE  = circum
);

COMMENT ON OPERATOR @-@ ( NONE, spoly ) IS
  'returns circumference of spherical polygon'; 


